{
  "name": "gatsby-plugin-material-symbols",
  "version": "0.0.0",
  "private": true,
  "author": "RYRY1002 <riley@riley.technology> (https://links.riley.technology)",
  "description": "Easily add Material Symbols to your Gatsby site, with various Gatsby-specific performance optimizations built-in.",
  "workspaces": [
    "plugin",
    "site"
  ],
  "scripts": {
    "develop:plugin": "yarn workspace plugin develop",
    "plugin:develop": "yarn workspace plugin develop",
    "develop:site": "yarn workspace site develop",
    "site:develop": "yarn workspace site develop",
    "build:plugin": "yarn workspace plugin build",
    "plugin:build": "yarn workspace plugin build",
    "build:site": "yarn workspace site build",
    "site:build": "yarn workspace site build",
    "build": "npm-run-all -s 'build:plugin' 'build:site'",
    "serve": "yarn workspace site serve",
    "serve:site": "yarn workspace site serve",
    "lint": "eslint . --ext .ts,.tsx,.js",
    "lint:fix": "yarn lint --fix",
    "test": "yarn workspace plugin test",
    "clean": "npm-run-all -p clean:plugin clean:site",
    "clean:plugin": "yarn workspace plugin clean",
    "plugin:clean": "yarn workspace plugin clean",
    "clean:site": "yarn workspace site clean",
    "site:clean": "yarn workspace site clean"
  },
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3"
  },
  "packageManager": "yarn@3.3.1"
}
